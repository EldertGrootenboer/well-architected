{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml",
          "**/toc.yml",
          "**/index.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.css",
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif",
          "**/*.svg",
          "**/*.js",
          "**/*.json",
          "**/*.docx",
          "feed.atom"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/azure/well-architected/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/well-architected",
      "brand": "azure",
      "uhfHeaderId": "azure",
      "titleSuffix": "Microsoft Azure Well-Architected Framework",
      "ms.service": "waf",
      "searchScope": [ "Azure", "Microsoft Azure Well-Architected Framework" ],    
      "ms.service": "waf"
    },
    "open_to_public_contributors": {
      "**/*.yml": false
    },
    "fileMetadata": {
        "ms.subservice": {
            "well-architected/resiliency/**/*.md": "waf-pillar-reliability",
            "well-architected/security/**/*.md": "waf-pillar-security",
            "well-architected/cost/**/*.md": "waf-pillar-optimization",
            "well-architected/scalability/**/*.md": "waf-pillar-efficiency",
            "well-architected/devops/**/*.md": "waf-pillar-excellence",
            "well-architected/mission-critical/**/*.md": "waf-workload-mission-critical",
            "well-architected/carrier-grade/**/*.md": "waf-workload-carrier-grade",
            "well-architected/hybrid/**/*.md": "waf-workload-hybrid",
            "well-architected/iot/**/*.md": "waf-workload-iot",
            "well-architected/sap/**/*.md": "waf-workload-sap",
            "well-architected/sustainability/**/*.md": "waf-workload-sustainability",
            "well-architected/services/**/*.md": "waf-service-guide"
         }
    },
    "template": [
      "docs.html"
    ],
    "dest": "well-architected",
    "markdownEngineName": "markdig"
  }
}
