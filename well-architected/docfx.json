{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml",
          "**/toc.yml",
          "**/index.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.css",
          "**/*.png",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.gif",
          "**/*.svg",
          "**/*.js",
          "**/*.json",
          "**/*.docx",
          "feed.atom"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/azure/well-architected/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/well-architected",
      "brand": "azure",
      "uhfHeaderId": "azure",
      "titleSuffix": "Microsoft Azure Well-Architected Framework",
      "searchScope": [ "Azure", "Microsoft Azure Well-Architected Framework" ]     
    },
    "open_to_public_contributors": {
      "**/*.yml": false
    },
    "fileMetadata": {},
    "template": [
      "docs.html"
    ],
    "dest": "well-architected",
    "markdownEngineName": "markdig"
  }
}
